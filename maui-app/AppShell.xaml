<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="d9.ucm.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:d9.ucm"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
        <ShellContent
        Title="Add Items"
        ContentTemplate="{DataTemplate local:AddItems}"
        Route="AddItems" />
        <ShellContent
        Title="Rank Items"
        ContentTemplate="{DataTemplate local:RankItems}"
        Route="RankItems" />
    </TabBar>

</Shell>
